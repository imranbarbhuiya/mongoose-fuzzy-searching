{
  "env": {
    "node": true,
    "commonjs": true,
    "es6": true
  },
  "extends": ["plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "no-underscore-dangle": "off",
    "no-plusplus": "off",
    "no-param-reassign": "off",
    "func-names": "off",
    "max-classes-per-file": "off"
  }
}
